---
import MainLayout from "../layouts/MainLayout.astro"
const res = await fetch(`${Astro.url}/api/coffee.json`)
const data = await res.json();
---

<MainLayout title ="Home Page" >		
	<p>Hello my first page</p>
	<ul>
		{data.map((coffee) => (
				<li>{coffee.title}</li>
		))}
	</ul>
</MainLayout>		
